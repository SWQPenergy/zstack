    420           * @return  none
    421           */
    422          static void SwqAPP_HandleKeys( uint8 shift, uint8 keys )
    423          {
    424            zAddrType_t dstAddr;
    425          
    426            // Shift is used to make each button/switch dual purpose.
    427            
    428            if ( shift )
    429            {
    430              if ( keys & HAL_KEY_SW_1 )
    431              {
    432              }
    433              if ( keys & HAL_KEY_SW_2 )
    434              {
    435              }
    436              if ( keys & HAL_KEY_SW_3 )
    437              {
    438              }
    439              if ( keys & HAL_KEY_SW_4 )
    440              {
    441              }
    442            
    443            }
    444            else
    445            {
    446              if ( keys & HAL_KEY_SW_1 )
    447              {
    448                // Since SW1 isn't used for anything else in this application...
    449          #if defined( SWITCH1_BIND )
    450                // we can use SW1 to simulate SW2 for devices that only have one switch,
    451                keys |= HAL_KEY_SW_2;
    452          #elif defined( SWITCH1_MATCH )
    453                // or use SW1 to simulate SW4 for devices that only have one switch
    454                keys |= HAL_KEY_SW_4;
    455          #endif
    456              }
    457          
    458              if ( keys & HAL_KEY_SW_2 )
    459              {
    460                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    461          
    462                // Initiate an End Device Bind Request for the mandatory endpoint
    463                dstAddr.addrMode = Addr16Bit;
    464                dstAddr.addr.shortAddr = 0x0000; // Coordinator
    465                ZDP_EndDeviceBindReq( &dstAddr, NLME_GetShortAddr(),
    466                                      SwqAPP_epDesc.endPoint,
    467                                      GENERICAPP_PROFID,
    468                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)SwqAPP_ClusterList,
    469                                      GENERICAPP_MAX_CLUSTERS, (cId_t *)SwqAPP_ClusterList,
    470                                      FALSE );
    471              }
    472          
    473              if ( keys & HAL_KEY_SW_3 )
    474              {
    475              }
    476             
    477              if ( keys & HAL_KEY_SW_4 )
    478              {
    479                HalLedSet ( HAL_LED_4, HAL_LED_MODE_OFF );
    480                // Initiate a Match Description Request (Service Discovery)
    481                dstAddr.addrMode = AddrBroadcast;
    482                dstAddr.addr.shortAddr = NWK_BROADCAST_SHORTADDR;
